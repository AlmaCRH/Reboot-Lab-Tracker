{
  "name": "front",
  "private": true,
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "dev": "concurrently \"vite\" \"wait-on http://localhost:3000 && electron ./src/main/main.js \"",
    "build": "vite build",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "preview": "vite preview",
    "start": "vite build && electron ./src/main/main.js"
  },
  "build": {
    "appId": "reboot.tracker.app",
    "productName": "Lab Tracker by Reboot Academy",
    "directories": {
      "output": "dist_electron"
    },
    "files": [
      "dist/**/*",
      "src/main/**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "public/icon.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "public/icon.icns"
    },
    "linux": {
      "target": "AppImage",
      "icon": "public/icon.png"
    }
  },
  "dependencies": {
    "@google-cloud/local-auth": "^2.1.0",
    "@octokit/app": "^15.1.0",
    "@octokit/webhooks": "^13.3.0",
    "agent-base": "^6.0.2",
    "arrify": "^2.0.1",
    "axios": "^1.7.7",
    "base64-js": "^1.5.1",
    "bcrypt": "^5.1.1",
    "before-after-hook": "^3.0.2",
    "bignumber.js": "^9.1.2",
    "body-parser": "^1.20.2",
    "buffer-equal-constant-time": "^1.0.1",
    "call-bind": "^1.0.7",
    "concurrently": "^8.2.2",
    "debug": "^4.3.6",
    "define-data-property": "^1.1.4",
    "dotenv": "^16.4.5",
    "ecdsa-sig-formatter": "^1.0.11",
    "electron-builder": "^25.0.5",
    "encoding": "^0.1.13",
    "es-define-property": "^1.0.0",
    "es-errors": "^1.3.0",
    "extend": "^3.0.2",
    "fast-text-encoding": "^1.0.6",
    "function-bind": "^1.1.2",
    "gaxios": "^5.1.3",
    "gcp-metadata": "^5.3.0",
    "get-intrinsic": "^1.2.4",
    "google-auth-library": "^8.9.0",
    "google-p12-pem": "^4.0.1",
    "googleapis": "^105.0.0",
    "googleapis-common": "^6.0.4",
    "gopd": "^1.0.1",
    "gtoken": "^6.1.2",
    "has-property-descriptors": "^1.0.2",
    "has-proto": "^1.0.3",
    "has-symbols": "^1.0.3",
    "hasown": "^2.0.2",
    "https-proxy-agent": "^5.0.1",
    "iconv-lite": "^0.6.3",
    "is-docker": "^2.2.1",
    "is-stream": "^2.0.1",
    "is-wsl": "^2.2.0",
    "json-bigint": "^1.0.0",
    "jwa": "^2.0.0",
    "jws": "^4.0.0",
    "lru-cache": "^10.4.3",
    "ms": "^2.1.2",
    "node-fetch": "^2.7.0",
    "node-forge": "^1.3.1",
    "object-inspect": "^1.13.2",
    "open": "^7.4.2",
    "qs": "^6.13.0",
    "safe-buffer": "^5.2.1",
    "safer-buffer": "^2.1.2",
    "server-destroy": "^1.0.1",
    "set-function-length": "^1.2.2",
    "side-channel": "^1.0.6",
    "socket.io": "^4.7.5",
    "tr46": "^0.0.3",
    "universal-github-app-jwt": "^2.2.0",
    "universal-user-agent": "^7.0.2",
    "url-template": "^2.0.8",
